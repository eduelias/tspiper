{
  "name": "tspiper",
  "version": "0.1.0",
  "repository": "https://github.com/eduelias/tspiper",
  "description": "It's a pipe manager",
  "main": "dist/index.js",
  "scripts": {
    "gb": "barrelsby --directory src --delete --location branch --singleQuotes --structure flat",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "author": "Eduardo Elias Saleh",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "barrelsby": "^1.0.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^3.2.2"
  }
}
